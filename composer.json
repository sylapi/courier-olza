{
    "name": "sylapi/courier-olza",
    "type": "library",
    "description": "Courier OLZA: GLS, DPD, PPL, Post",
    "keywords": [
        "courier",
        "olza",
        "shipping"
    ],
    "homepage": "https://github.com/sylapi/courier-olza",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Zapotoczny"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "sylapi/courier": "@dev",
        "develart-projects/olza-logisctic-api-client": "1.1.*"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../courier/"
        }
    ],    
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^0.12.77"
    },
    "autoload": {
        "psr-4": { "Sylapi\\Courier\\Olza\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Sylapi\\Courier\\Olza\\Tests\\": "tests/" }
    },     
    "prefer-stable": false
}
